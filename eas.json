{
    "cli": {
        "version": ">= 3.5.2",
        "promptToConfigurePushNotifications": false
    },
    "build": {
        "development": {
            "developmentClient": true,
            "distribution": "internal",
            "ios": {
                "resourceClass": "m-medium"
                "env": {
                    "FIREBASE_APP_CHECK_DEBUG_TOKEN": process.env.IOS_APP_CHECK_DEBUG_TOKEN,
                }
            }
            ,"android": {
                "env": {
                    "FIREBASE_APP_CHECK_DEBUG_TOKEN": process.env.ANDROID_APP_CHECK_DEBUG_TOKEN,
                }
            }
        },
        "preview": {
            "distribution": "internal",
            "ios": {
                "resourceClass": "m-medium"
            }
        },
        "production": {
            "ios": {
                "resourceClass": "m-medium"
            }
        }
    },
    "submit": {
        "production": {}
    }
}
